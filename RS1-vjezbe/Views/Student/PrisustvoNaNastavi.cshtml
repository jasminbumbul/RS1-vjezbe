
@model StudentPrisustvoNaNastaviVM 
@{
    Layout = null;
}

<h3>Ime studenta : @Model.ImeStudenta</h3>


<table class="table table-bordered">
    <thead>
        <tr>
            <th>Predmet</th>
            <th>Datum</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var x in Model.Zapisi)
        {
            <tr>
                <td>@x.Predmet</td>
                <td>@x.Datum</td>
            </tr>
        }
    </tbody>
</table>